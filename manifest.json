{
    "manifest_version": 3,
    "name": "Coffee Break",
    "version": "1.2",
    "description": "A study or work timer with rewards and themes!",
    "permissions": ["storage"],
    "icons": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
      }
    },
    "web_accessible_resources": [
      {
        "resources": ["assets/*.png", "assets/*.mp3"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  